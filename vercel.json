{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      },
      {
        "src": "api/cron.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      },
      {
        "src": "/api/cron",
        "dest": "/api/cron.js"
      }
    ],
    "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 10 * * *"
    }
  ]
  }
  